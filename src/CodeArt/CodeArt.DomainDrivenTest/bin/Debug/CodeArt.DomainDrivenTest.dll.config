<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="codeArt" type="CodeArt.Configuration, CodeArt"/>
    <section name="codeArt.rabbitMQ" type="CodeArt.RabbitMQ.RabbitMQConfiguration, CodeArt.RabbitMQ" />
    <section name="codeArt.serviceModel" type="CodeArt.ServiceModel.ServiceModelConfiguration, CodeArt.ServiceModel"/>
  </configSections>
  <connectionStrings>
    <add name="db-default" connectionString="server=127.0.0.1;database=DomainDriven;uid=sa;pwd=qjs_Q!1;"/>
  </connectionStrings>

  <codeArt />
  <codeArt.serviceModel>
    <client identityName="test"/>
  </codeArt.serviceModel>

  <codeArt.rabbitMQ>
    <policyGroup>
      <!-- name:策略名称 -->
      <!-- server:rabbitMQ的宿主地址，可以使用标准格式host：port（例如host = myhost.com：15672）。如果省略端口号，则使用默认的AMQP端口（15672）。 -->
      <!-- virtualHost:虚拟主机名称 -->
      <policy name="event" host="127.0.0.1" virtualHost="test" userName="sa" password="Q!1" />
      <policy name="rpc" host="127.0.0.1" virtualHost="test" userName="sa" password="Q!1" />
    </policyGroup>
  </codeArt.rabbitMQ>
  

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/></startup></configuration>
